<?xml version="1.0" encoding="utf-8" ?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>

    <form-beans>
        <form-bean name="accountForm" type="org.csu.struts1demo.domain.Account" />
    </form-beans>
    <action-mappings>
        <action path="/login" name="accountForm" type="org.csu.struts1demo.web.LoginAction">
            <forward name="input" path="/WEB-INF/jsp/login.jsp" />
            <forward name="success" path="/WEB-INF/jsp/main.jsp" />
        </action>
    </action-mappings>
</struts-config>